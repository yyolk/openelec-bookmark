{
  "name": "GitHub",
  "description": "Online project hosting using Git.",
  "default_locale" : "en",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "permissions": [],
  "app": {
    "launch": {
      "container": "tab",
      "web_url": "https://github.com"
    },
    "web_content": {
      "enabled": true,
      "origin": "https://github.com"
    }    
  }
}
